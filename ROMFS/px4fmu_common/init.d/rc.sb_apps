#!nsh
#
# Standard apps for autonomous sailing:
#

#
# start the autonomous_sailing.c script:
#
autonomous_sailing start

#
# start the ekf_att_pos_estimator.cpp script:
#
ekf_att_pos_estimator start

#
# start the parser_200WX.c script:
#
parser_200WX start

#
# start the path_planning.c script:
# Use sleep to give parser_200WX time to start up, before Pathplanning is started 
usleep 100000
path_planning start

#
# start the mission_planning.c script:
#
mission_planning start